package com.index;

import java.util.Scanner;

public class Theatre {

	public static void main(String[] args) {
		Scanner a=new Scanner(System.in);
				int screen=a.nextInt();
		int timings=a.nextInt();
		switch(screen)
		{
			case 1:
				switch(timings)
				{
					case 7:
						System.out.println("The show timing is 7-10,movie name is Thuglife");
						break;
					case 10:
						System.out.println("The show timing is 10-1,movie is DD");
						break;
					case 1:
						System.out.println("the show timing is 1-4,movie name is anaconda");
						break;
						default:
							System.out.println("Invalid input");		
				}
		}

	}

}
